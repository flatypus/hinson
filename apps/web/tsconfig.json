{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "plugins": [{ "name": "next" }],
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
  
}
